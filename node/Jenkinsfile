pipeline {
    agent { docker { image 'node:6.3'  }  }
    stages {
        stage('build') {
            steps {
                sh 'npm --version'
            }
        }
        stage('Test node') {
            steps {
                sh 'npm list'
            }
        }
        stage('PWD') {
            steps {
                sh 'echo $PWD'
            }
        }
    }
    post {
        always {
            echo 'Post step!'
        }
    }
}
